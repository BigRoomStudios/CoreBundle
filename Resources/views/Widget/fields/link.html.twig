{% set params = {} %}

{% for key, param in field.route.params %}
	{% if key is numeric %}
		{% set params = update(params, param, item[param]) %}
	{% else %}
		{% set params = update(params, key, item[param]) %}
	{% endif %}
{% endfor %}

<a class="{{ field.class }}" {% if field.nav is defined %}data-route="{{ field.route.name }}"{% endif %} href="{{ path(field.route.name, params) }}">{% if field.label is defined %}{{ field.label | trans }}{% else %}{{ item[key] }}{% endif %}</a>